{
  "expo": {
    "name": "AgendaFacil",
    "slug": "agendamento-servicos",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/images/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.agendamento.servicos",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.agendamento.servicos",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#c470d1"
      },
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#c470d1",
      "androidMode": "collapse",
      "androidCollapsedTitle": "ðŸ“… AgendaFÃ¡cil"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#c470d1",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#c470d1",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "scheme": "agendamento",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "53e8888c-f828-41a7-8fd3-189f68b584c3"
      }
    }
  }
}
